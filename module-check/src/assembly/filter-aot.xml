<assembly>
  <id>default-jar</id>
  <formats>
    <format>jar</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  <fileSets>
    <fileSet>
      <directory>src/main/clojure</directory>
      <outputDirectory>/</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>src/main/cljs</directory>
      <outputDirectory>/</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>target/classes/clojure/core/typed</directory>
      <outputDirectory>clojure/core/typed</outputDirectory>
      <excludes>
        <exclude>**/test/**</exclude>
      </excludes>
    </fileSet>
  </fileSets>
</assembly>
